[
  {
    "kernelSource": {
      "name": "Xiaomi8SE",
      "repo": "https://github.com/Shakawazu/android_kernel_xiaomi_sdm710",
      "branch": "10.0-caf",
      "device": "sirius",
      "defconfig": "sirius_defconfig"
    },
    "toolchains": [
      {
        "name": "clang",
    "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/android-11.0.0_r48.tar.gz",
    "binaryEnv": ["./bin"]
      },
      {
        "name": "gcc",
    "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/android-11.0.0_r48.tar.gz",
    "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-",
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
